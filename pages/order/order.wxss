/* pages/make/make.wxss */
page{
  background: #f2f2f2;   
  height: 900rpx; 
}
swiper{
  height: 900rpx;
}
.contain{
  /* background: url("http://alpha-jc.pinyer.com/demo/images/bg.jpg"); */
  background-size:100% 100%; 
  width:80%;
  margin:  auto;
  margin-top: 100rpx;
  height: 700rpx;
  background: #fff;
  position: relative;
}
#right{
width: 100rpx;
background: url("http://cqingt.oss-cn-shenzhen.aliyuncs.com/image/cup-right2.png");
background-size: 100% 100%;
z-index: 123;
position: absolute;
right: 0;
height: 700rpx;
}
#left{
width: 100rpx;
background: url("http://cqingt.oss-cn-shenzhen.aliyuncs.com/image/cup-left2.png");
background-size: 100% 100%;
z-index: 123;
position: absolute;
left: 0;
height: 700rpx;
}
#top{
width:96%;
height:1rpx;
position:absolute;
/* top:6px; */
box-shadow:0px 2px 3px #ccc;
z-index:12;
left:50%;
margin-left:-48%;
top:-1px;
}
#bottom{
width:68%;
height:2rpx;
position:absolute;
bottom:1px;
box-shadow:1px 2px 3px #ccc;
z-index:12345;
left:50%;
margin-left:-34%;
}
.top{
  width: 100%;
  display: flex;
}
.top_img{
  height:140rpx;
  width:140rpx;
  padding:0rpx;
  border:1px solid #aaa;
  border-radius:140rpx;
  overflow:hidden;
  box-shadow:0 0 10px #aaa;
}
.top>view{
  flex: 1;
  text-align: center;
}
.top_process{
  display: flex;
  justify-content: center;
  font-size: 0.65em;
  
  border-radius: 20rpx;
}
.top_process_border{
  width:60%;
  margin:0 auto;
  border:2px black solid;
  z-index:124;
  font-size:0.8em;
  border-radius:20rpx;
  height:60%;
  display:flex;
  align-items:center;
  justify-content:center;
  margin-top:20rpx;
  font-weight: bloder;
}
.top_status{
  display: flex;
  justify-content: center;
  font-size: 0.6em;
  margin-top: 15rpx;
  line-height: 57rpx;
  z-index: 124;
  font-weight: bloder;
  padding-left: 100rpx;
}
.top_img image{
  width:120rpx;
  height:120rpx;
  margin-top: -60rpx;
}
.text{
  width: 100%;
  font-size: 0.65em;
  margin-top: 100px;
  color:#A3A3A3;
}
.text_top{
  text-align: center;
}
.text_bot{
  text-align: center;
}
.btn{
  width: 50%;
  height: 50rpx;
  line-height: 50rpx;
  margin: 0 auto;
  font-size: 0.7em;
  text-align: center;
  background: black;
  color: #fff;
  border-radius: 90px;
  margin-top: 80px;
}
.recommend{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}
.re_left{
  flex: 1;
  height: 1px;
  background: black;
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-top: 18rpx;
}
.re_center{
  font-size: 0.7em;
  font-weight: bold;

}
.re_right{
  flex: 1;
  height: 1px;
  background: black;
  margin-right: 30rpx;
  margin-left: 30rpx;
  margin-top: 18rpx;
}
/* 底部左右滑动 */
.wineNameitemNameText{
  font-size: 0.6em;
}
.wineNameitemText{
  font-size: 0.6em;
}
